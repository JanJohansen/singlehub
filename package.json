{
  "name": "singlehub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run build-ts && npm run copy-static-assets",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon dist/app.js\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "copy-static-assets": "node bin/copyStaticAssets.js",
    "postinstall": "bower install --allow-root && npm run build",
    "check-updates": "ncu -u && ncu -m bower -u"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "express-validator": "^3.1.2",
    "jade": "^1.11.0",
    "lodash": "^4.17.4",
    "logplease": "^1.2.13",
    "morgan": "^1.8.1",
    "mqtt": "^2.4.0",
    "nedb": "^1.8.0",
    "serialport": "^4.0.7",
    "serve-favicon": "^2.3.2",
    "socket.io": "^1.7.2",
    "source-map-support": "^0.4.11",
    "split": "^1.0.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "@types/body-parser": "^0.0.34",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.35",
    "@types/express-validator": "^2.20.33",
    "@types/handlebars": "^4.0.31",
    "@types/jquery": "^2.0.39",
    "@types/jquery.noty": "^2.0.28",
    "@types/lodash": "^4.14.52",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.32",
    "@types/mqtt": "0.0.33",
    "@types/nedb": "^0.0.31",
    "@types/node": "^7.0.0",
    "@types/serialport": "^4.0.7",
    "@types/serve-favicon": "^2.2.28",
    "@types/shelljs": "^0.7.2",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.29",
    "@types/vis": "^4.17.3",
    "concurrently": "^3.5.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "shelljs": "^0.7.8",
    "typescript": "^2.4.1"
  }
}